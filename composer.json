{
    "name": "kazuyafaizad/laravel-gamify",
    "description": "Add Gamification in laravel app with point and badges support",
    "homepage": "https://github.com/ansezz/laravel-gamify",
    "type": "vcs",
    "license": "MIT",
    "keywords": [
        "ansezz",
        "laravel",
        "achivement",
        "gamify",
        "gamification",
        "reputation",
        "points",
        "badge",
        "reward"
    ],
    "authors": [
        {
            "name": "Anass Ez-zouaine",
            "email": "ansezzouaine@gmail.com"
        }
    ],
      "require": {
    "php": "^7.3|^8.0",
    "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8|^4.0|^5.0|^7.0",
    "mockery/mockery": "^0.9.4 || ~1.0",
    "phpunit/phpunit": "~8.5|^9.0"
  },
    "autoload": {
        "psr-4": {
            "Kazuyafaizad\\Gamify\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kazuyafaizad\\Gamify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kazuyafaizad\\Gamify\\GamifyServiceProvider"
            ],
            "aliases": {
                "Gamify": "Kazuyafaizad\\Gamify\\Facades\\GamifyFacade"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
